fmod IBOS-AUX-NONFVP is
  pr IBOS-PREDS-FVP .
  pr ATTRIBUTE-NONFVP-EXT .

  var W N M : Nat .
  var Z     : NzNat .

  var C C' : Configuration .
  var NC : NeConfiguration .
  var NWC : NetWebProcConfig .

  var At At' : Attribute .
