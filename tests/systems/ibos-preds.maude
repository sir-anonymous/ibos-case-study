  --- 3. Add partially FVP predicate for checking if an object with the named ID is in the configuration
  op in-conf? : ProcessId Configuration -> Bool [metadata "116 true"] .
  eq in-conf?(P, < P  | A > C) = true [variant] .
  --- recursive cases
 ceq in-conf?(P, < P' | A > C) = in-conf?(P,C) if P ~p P' = false .
  --- base cases
  eq in-conf?(AP, C)           = false .
  eq in-conf?(P?, none)        = false .
  --- lemmas
 ceq in-conf?(NP, C)           = false if fresh-np-id?(NP,C) = true .
 ceq in-conf?(WP, C)           = false if fresh-wp-id?(WP,C) = true .
  --- attribute canonicalizations
  eq in-conf?(P?, < P | toKernel(T:NeMessageList),   A > C) = in-conf?(P?, < P | toKernel(mt),          A > C) .
  eq in-conf?(P?, < P | fromKernel(F:NeMessageList), A > C) = in-conf?(P?, < P | fromKernel(mt),        A > C) .
  eq in-conf?(P?, < P | returnTo(W:ConcWebProcId),   A > C) = in-conf?(P?, < P | returnTo(ui),          A > C) .
  eq in-conf?(P?, < P | returnTo(N:ConcNetProcId),   A > C) = in-conf?(P?, < P | returnTo(ui),          A > C) .
  eq in-conf?(P?, < P | mem-in(L:Label),             A > C) = in-conf?(P?, < P | mem-in(nolabel),       A > C) .
  eq in-conf?(P?, < P | mem-out(L:Label),            A > C) = in-conf?(P?, < P | mem-out(nolabel),      A > C) .
  eq in-conf?(P?, < P | in(L:NeLabelList),           A > C) = in-conf?(P?, < P | in(mtLL),              A > C) .
  eq in-conf?(P?, < P | out(L:NeLabelList),          A > C) = in-conf?(P?, < P | out(mtLL),             A > C) .
  eq in-conf?(P?, < P | URL(U:NonBlankLabel),        A > C) = in-conf?(P?, < P | URL(about-blank),      A > C) .
  eq in-conf?(P?, < P | rendered(R:NonBlankLabel),   A > C) = in-conf?(P?, < P | rendered(about-blank), A > C) .
  eq in-conf?(P?, < P | loading(on),                 A > C) = in-conf?(P?, < P | loading(off),          A > C) .

  --- 4. Add partially FVP predicate for checking for duplicate object IDs in the configuration
  op conf-dupl? : Configuration -> Bool [metadata "133 true"] .
  eq conf-dupl?(< P | A  > < P | A' > C) = true [variant] .
  eq conf-dupl?(none) = false .
 ceq conf-dupl?(< P | A > C) = conf-dupl?(C) if in-conf?(P,C) = false .
  ---
  eq conf-dupl?(< P | toKernel(T:NeMessageList),   A > C) = conf-dupl?(< P | toKernel(mt),          A > C) .
  eq conf-dupl?(< P | fromKernel(F:NeMessageList), A > C) = conf-dupl?(< P | fromKernel(mt),        A > C) .
  eq conf-dupl?(< P | returnTo(W:ConcWebProcId),   A > C) = conf-dupl?(< P | returnTo(ui),          A > C) .
  eq conf-dupl?(< P | returnTo(N:ConcNetProcId),   A > C) = conf-dupl?(< P | returnTo(ui),          A > C) .
  eq conf-dupl?(< P | mem-in(L:Label),             A > C) = conf-dupl?(< P | mem-in(nolabel),       A > C) .
  eq conf-dupl?(< P | mem-out(L:Label),            A > C) = conf-dupl?(< P | mem-out(nolabel),      A > C) .
  eq conf-dupl?(< P | in(L:NeLabelList),           A > C) = conf-dupl?(< P | in(mtLL),              A > C) .
  eq conf-dupl?(< P | out(L:NeLabelList),          A > C) = conf-dupl?(< P | out(mtLL),             A > C) .
  eq conf-dupl?(< P | URL(U:NonBlankLabel),        A > C) = conf-dupl?(< P | URL(about-blank),      A > C) .
  eq conf-dupl?(< P | rendered(R:NonBlankLabel),   A > C) = conf-dupl?(< P | rendered(about-blank), A > C) .
  eq conf-dupl?(< P | loading(on),                 A > C) = conf-dupl?(< P | loading(off),          A > C) .

  op in-wis? : WebProcInfo WebProcInfoSet -> Bool [metadata "130 true"] .
  eq in-wis?(pi(WP,IN),pi(WP ,IN) ,wp WIS) = true [variant] .
 ceq in-wis?(pi(WP,IN),pi(WP',IN'),wp WIS) = in-wis?(pi(WP,IN),WIS) if WP ~p WP' and IN ~l IN' = false .
  eq in-wis?(pi(WP,IN),mtWPIS) = false .

  op goodCtxt? : NetWebProcConfig WebProcInfoSet NetProcInfoSet -> Bool [metadata "132"] .
  eq goodCtxt?(NNWC,NWIS,  NNIS  ) = true [variant] .
  eq goodCtxt?(NNWC,NWIS,  mtNPIS) = true [variant] .
  eq goodCtxt?(none,mtWPIS,mtNPIS) = true [variant] .
