  eq webproc-req-attrs(returnTo(W:ConcWebProcId),   A) = webproc-req-attrs(returnTo(ui),        A) .
  eq webproc-req-attrs(returnTo(N:ConcNetProcId),   A) = webproc-req-attrs(returnTo(ui),        A) .
  eq webproc-req-attrs(mem-in(L:Label),             A) = webproc-req-attrs(mem-in(nolabel),     A) .
  eq webproc-req-attrs(mem-out(L:Label),            A) = webproc-req-attrs(mem-out(nolabel),    A) .
