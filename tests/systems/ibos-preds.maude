  eq netproc-req-attrs(URL(U:NonBlankLabel),        A) = netproc-req-attrs(URL(about-blank),      A) .
  eq netproc-req-attrs(rendered(R:NonBlankLabel),   A) = netproc-req-attrs(rendered(about-blank), A) .
